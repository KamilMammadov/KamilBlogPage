@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using KamilBlog.Database.Models
@model About


<div class="col-lg-12">
    <div class="card">
        <div class="card-header">
            <h4 class="card-title">Blogs</h4>
        </div><!--end card-header-->
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-striped mb-0">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>ContentName</th>
                           
                            <th class="text-end">Actions</th>
                        </tr>
                    </thead>
                    <tbody>
                      
                            <tr>
                                <td>@Model.Name</td>
                            <td>@Model.ContentName</td>
                              
                                <td class="text-end">
                                <a asp-route="admin-about-update" asp-route-id="@Model.Id"><i class="las la-pen text-secondary font-16"></i></a>
                                  
                                </td>
                            </tr>
                        
                    </tbody>
                </table><!--end /table-->
            </div><!--end /tableresponsive-->
        </div><!--end card-body-->
    </div><!--end card-->
</div>